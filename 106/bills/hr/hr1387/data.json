  "introduced_at": "1999-04-13", 
  "number": "1387", 
  "official_title": "To amend title 38, United States Code, to provide for Government furnished headstones or markers for the marked graves of veterans.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "6", 
    "name": "Johnson, Nancy L.", 
