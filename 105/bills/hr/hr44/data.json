  "introduced_at": "1997-01-07", 
  "number": "44", 
  "official_title": "To amend title 10, United States Code, to provide limited authority for concurrent payment of retired pay and veterans' disability compensation for certain disabled veterans.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "district": "9", 
    "name": "Bilirakis, Michael", 
    "state": "FL", 
    "thomas_id": "00089", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "1997-01-07", 
  "subjects": [
    "Armed forces and national security", 
    "Disabled", 
    "Labor and employment", 
    "Military pensions", 
    "Veterans' disability compensation"
  ], 
  "subjects_top_term": "Armed forces and national security", 
  "summary": {
    "as": "Introduced", 
    "date": "1997-01-07", 
    "text": "States that a prohibition on the concurrent award of veterans' disability compensation and military retired pay shall not apply to a person who has a service-connected disability if: (1) the person completed at least 20 years of retirement-creditable military service; and (2) the disability was incurred or aggravated in the line of duty and is rated at not less than 70 percent."
  }, 
