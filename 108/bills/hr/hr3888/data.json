  "status_at": "2004-03-03", 
  "subjects": [
    "Business records", 
    "Commerce", 
    "Economics and public finance", 
    "Employee benefit plans", 
    "Federally-guaranteed loans", 
    "Foreign trade and international finance", 
    "Government lending", 
